/* For Extension front page */

#region-content {
    margin:0;
    padding-top: 0 !important;
}

#portlets-above {
    display: block;
}

#portlets-above .portletHeader {
    font-size:1.15em;
    font-weight:bold;
    padding:0.5em 0 0.25em 0;
    text-transform:none;
    background-position:left top;
    background-repeat:no-repeat;
    margin-left: 5px;
    letter-spacing: -0.05em;
}

#portlets-above .portlet {
    min-height: 32em;
}

#portlets-above dl.portlet:first-child .portletHeader {
    padding-top:55px;
}

#portal-column-two .portletHeader a {
    color:#2F4977;
}

#portal-column-two .portlet {
    border-bottom: none;
}

.documentContent dd a {
    border-bottom: none;
}


}

#portal-column-one .portletContact .portletItem p strong,
#portal-column-two .portletContact .portletItem p strong,
#portlets-mobile .portletContact .portletItem p strong {
    font-weight: normal;
}

#portal-column-two .portletContact .portletItem {
    padding: 0;
}

#portal-column-one .portletStaticText img,
#portlets-mobile .portletStaticText img {
    width: 166px;
    display: block;
    margin-top: 0.9375em;
}



/* Portal-column-one Navigation */

#portal-column-one .portletNavigationHeader,
#navigation-mobile .portletNavigationHeader {
    border-bottom: 0;
    background-color: &dtml-leftNavTitleBackground;;
    color: &dtml-backgroundColor;;

#rightcolumn dl.portletfeedmixer span.summary,
#portal-column-one dl.portletfeedmixer div.summary,
#portal-column-two dl.portletfeedmixer div.summary,
#rightcolumn dl.portletfeedmixer div.summary,
#portlets-mobile dl.portletfeedmixer span.summary,

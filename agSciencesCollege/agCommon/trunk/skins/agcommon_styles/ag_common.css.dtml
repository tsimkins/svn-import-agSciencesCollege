*/

/* YOUR CSS RULES START HERE */

/* = body =================================================== */

body {
    background-color: &dtml-bodyBackgroundColor;;
    background-image:url('&dtml-portal_url;/bodyBackground.png');
    color: #000000;
    background-repeat: repeat-x;
    background-position: left top;
    font-size: 100% !important;
}


/* = visual-portal-wrapper ================================== */

#visual-portal-wrapper {
    background-color: &dtml-backgroundColor;;
    margin: 0 auto;
    padding: 0 6px 0.375em 6px;
    max-width: &dtml-maxWidth;;
    min-width: &dtml-minWidth;;
    /* Drop Shadow */
    -moz-box-shadow: 0 0 0.625em #333333;
    -webkit-box-shadow: 0 0 0.625em #333333;
    box-shadow: 0 0 0.625em #333333;
}


/* = portal-top ============================================= */

#portal-top {
    margin: 0;
    border-left: 1px solid #808080;
    border-right: 1px solid #808080;
    width: 99.776785714285714%; /* 894/896 */
    border-collapse: separate;
    border-spacing: 0;
    display: table;
}


/* = portal-header ========================================= */

#portal-header {
    background: &dtml-backgroundColor; url(&dtml-portal_url;/&dtml-logoName;) no-repeat left 0.6875em;
    margin: 0.0625em 0 0 0;
    padding: 0;
    position: relative;
    min-height: 4.6875em;
}

#portal-header a {
    border-width: 0;
    text-decoration: none;
}


/* = portal-searchbox ====================================== */

#portal-searchbox {
    margin: 0.25em;
    padding: 0 0.25em 0 0;
    position: absolute;
    right: 0;
    top: 1px;
    clear: none;
    float: none;
}

.searchSection {
    display: none;
}

#portal-searchbox input.searchButton {
    background-color: transparent;
    background-image: none;
    border: none;
    padding: 0.125em 0 0 0;
    margin: 0 0 0 -0.125em;
    vertical-align: middle;
}

#portal-searchbox label.hiddenStructure {
    display: none;
}

#portal-searchbox #SearchableText {
    font-size: 0.6875em;
    padding: 0.1875em 0.125em;
    border: 1px solid #999999;
    vertical-align: middle;
}

#portal-searchbox #searchSource {
    font-size: 0.6875em;
    padding: 0.125em;
    margin-top: 0.0625em;
    vertical-align: middle;
}


/* = logo-psu ============================================== */

#logo-psu {
    height: 55px;
    width: 90px;
    top: 15px;
    left: 5px;
    position: absolute;
    display: block;
}


/* = logo-college ========================================== */

#logo-college {
    display: block;
    height: 33px;
    left: 101px;
    position: absolute;
    top: 38px;
    width: 56px;
}


/* = logo-print ============================================ */

#logo-print {
    display: none;
}


/* = logo-site ============================================= */

#logo-site {
    font-family: Arial,Helvetica,sans-serif;
    font-size: &dtml-siteLogoFontSize;;
    margin-left: 188px;
    letter-spacing: 0.02em;
    position: static;
    margin-top: &dtml-siteLogoTopMargin;;
    color: #1144AA;
    display: block;
}


/* = portal-breadcrumbs ==================================== */

#portal-breadcrumbs {
    background-color: &dtml-insideBreadcrumbBackgroundColor;;
    color: &dtml-insideBreadcrumbTextColor;;
    font-weight: bold;
    letter-spacing: 0.025em;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0.375em;
    padding-bottom: 0.375em;
    border-bottom: 0;
    margin: 0 1.375em 0 1.375em;
    line-height: 1.125em;
}

#portal-breadcrumbs .breadcrumbSeparator {
    color: &dtml-insideBreadcrumbSeparatorColor;;
}

#portal-breadcrumbs>span {
    font-size: 0.625em;
}

#portal-breadcrumbs a {
    color: &dtml-insideBreadcrumbLinkColor;;
    font-weight: normal;
}

/* = portal-columns ======================================== */

#portal-columns {
    margin: 0;
    padding: 0;
    background-color: transparent;
    border-collapse: separate;
    border-spacing: 0;
    display: table;
    width: 99.776785714285714%; /* 894/896 */
    border-width: 0 1px;
    border-style: solid;
    border-color: #808080;
}

/* = portal-column-one ===================================== */

#portal-column-one {
    background: &dtml-leftNavBackgroundColor; url(&dtml-portal_url;/&dtml-bottomLeftColumnBackgroundImage;) no-repeat center bottom;
    border-color: &dtml-leftNavBorderColor;;
    border-style: solid;
    border-width: 0 1px 0 0;
    width: 21.027787948637817%;   /* 187/892 */
    padding: 0.336322869955157%;  /* 3/892 */
    margin: 0;
    vertical-align: top;
    display: table-cell;

}

#portal-column-one .visualPadding {
    padding: 0;
}

#portal-column-one-visualPadding.visualPadding {
    margin-bottom: 9.375em;
    padding: 0;
}

#portal-column-one a {
    color: #265FCF;
}

#portal-column-one a:hover {
    color: #003399;
}


/* ----- portlets ------------------------------------------ */

#content .portlet dd a:visited {
    color: #265FCF;
}

#portal-column-one .portletWrapper,
#portlets-mobile .portletWrapper {
    margin: 0 3.20855614973262% 1.25em 3.20855614973262%;  /* 6/187 */
    width: 93.58288770053476%;
    padding: 0;
    font-size: 0.71875em;
}


#portlets-mobile .portletWrapper {
    margin: 0 1.60427807486631% 1.25em 1.60427807486631%;  /* 6/187 */
}

#navigation-mobile .portletWrapper {
    margin: 0 -1.7941593898646% 1.25em -1.7941593898646%;
}

#portal-column-one .portletHeader,
#portlets-mobile .portletHeader,
#navigation-mobile .portletHeader {
    background-color: transparent;
    color: &dtml-insideRightPortletHeaderTextColor;;
    border-color: &dtml-insideRightPortletHeaderTextColor;;
    border-width: 0 0 1px 0;
    padding: 0.1875em 0 0.125em 0;
    font-weight: bold;
    line-height: 1.25em;
    font-family: Arial, Helvetica, Helv, sans-serif;
    font-size: 1.1875em;
}

#portal-column-one .portlet,
#portlets-mobile .portlet {
    border: none;
    background-color: transparent;
}

#portal-column-one .portletStaticText .portletItem,
#portlets-mobile .portletStaticText .portletItem {
    margin-left: 0.3125em;
    margin-right: 0.3125em;
}

#portal-column-one .portletStaticText .portletItem h2,
#portal-column-two .portletStaticText .portletItem h2,
#portal-column-one .portletContact .portletItem h2,
#portal-column-two .portletContact .portletItem h2,
#portlets-mobile .portletStaticText .portletItem h2,
#portlets-mobile .portletContact .portletItem h2 {

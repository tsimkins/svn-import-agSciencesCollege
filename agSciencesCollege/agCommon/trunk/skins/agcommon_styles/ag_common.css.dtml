    padding: 0.9375em;
    font-size: 1em;
    border-bottom: 1px solid &dtml-leftNavBorderColor;;
    text-transform: none;
}

#portal-column-one .portletNavigationHeader a,
#navigation-mobile .portletNavigationHeader span {
    background-color: transparent;
    color: &dtml-leftNavTitleColor; !important;
    line-height: 1.4em;
    padding: 0;
    margin: 0;
    font-family: "Lucida Grande","Trebuchet MS",Tahoma,Verdana,sans-serif;
    font-weight: bold;

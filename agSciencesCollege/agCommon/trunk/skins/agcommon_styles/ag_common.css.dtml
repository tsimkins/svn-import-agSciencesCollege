#portlets-mobile .linkButton li.internal-link:hover,
#portal-column-one .linkButton li.internal-link:hover,
#portal-column-two .linkButton .portletItem li.internal-link:hover {
    border-color:<dtml-var expr="gradientEnd(startColor=leftButtonBackground_external)">;
}

/* = portal-column-content ================================= */

#portal-column-content {
    margin: 0;
    margin-bottom: 0;
    padding: 0;
    background-color: &dtml-backgroundColor;;
    vertical-align: top;
    display: table-cell;
    border: none;
    width: 100%;
}

#portal-column-content.sl {
    width: 78.299776286353468%; /* (894 - (187+3+3+1)) / 894 */
}

#portal-column-content.sr {
    margin-right: 0;
    width: 78.674972035794183%; /* (894 - (172.64575+9+9)) / 894 */
}

#portal-column-content.sl.sr {
    width: 56.974748322147651%; /* (894 - (172.64575+9+9+187+3+3+1)) / 894 */
}

/* = portal-top-navigation ================================= */

#portal-top-navigation {
    margin: 0.375em 0 0 0;
    padding: 0;
    background-color: &dtml-topnavigationBackgroundColor;;
    background-image:url('&dtml-portal_url;/topnav-gradientBackground.png?startColor=<dtml-var expr="topnavigationBackgroundColor.replace('#','')">&endColor=<dtml-var expr="topnavigationGradientBackground.replace('#','')">&height=20');
    background-repeat: repeat-x;
    background-position: bottom left;
    /* Fix bleed-through of image in Safari and Chrome */
    z-index: 10;
    position: relative;
    border-width: 0;
}

#portal-top-navigation.empty {
    height: 0.1875em;
}

#portal-top-navigation ul {
    display: block;
    text-align: left;
    margin: 0 auto;

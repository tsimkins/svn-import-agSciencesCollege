}


/* Link Buttons */

#portlets-mobile .linkButton,
#portal-column-one .linkButton,
#portal-column-two .linkButton {
    margin: 30px 0;
}

#portlets-mobile .linkButton ul,
#portal-column-one .linkButton ul,
#portal-column-two .linkButton ul {
    margin: 10px 0;
}

#portlets-mobile .linkButton li,
#portal-column-one .linkButton li,
#portal-column-two .linkButton .portletItem li {
    display: block;
    padding: 0;
    border: 2px solid <dtml-var expr="gradientEnd(startColor=leftButtonBackground_external, count=2)">;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    margin: 0 0 15px 0;
    list-style-type: none;
    list-style-image: none;
    background-color: &dtml-leftButtonBackground_external;;
    background-repeat: repeat-x;
    background-image: url('&dtml-portal_url;/leftbutton-gradientBackground.png?startColor=<dtml-var expr="leftButtonBackground_external.replace('#','')">&height=20');
    background-position: left bottom;
    -moz-box-shadow: 0 2px 2px #D1D6DF;
    -webkit-box-shadow: 0 2px 2px #D1D6DF;

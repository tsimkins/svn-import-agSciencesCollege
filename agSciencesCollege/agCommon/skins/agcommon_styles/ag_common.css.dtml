/* Thumbnail view workaround for addthis.  We're assuming that we don't use a right column on image pages. */

body.template-atct_album_view #rightcolumn {
	float: none;
}

body.template-atct_album_view #rightcolumn .portletWrapper {
	display: none;
}

body.template-atct_album_view #addthis {
	float: right;
	margin-bottom: 0px;
}

/* Summary view workaround for addthis.  We're assuming that we don't use a right column on image pages. */

body.template-folder_summary_view #rightcolumn {
	float: none;
}

body.template-folder_summary_view #rightcolumn .portletWrapper {
	display: none;
}

body.template-folder_summary_view #addthis {
	float: right;
	margin-bottom: 0px;
}

/* Don't show addthis on homepage */ 
body.template-document_homepage_view #addthis {
	display: none;
}

#portal-column-one div.leftColumnButtons a {
	text-decoration: none;
	color: &dtml-leftLinkTextColor;;
}

#portal-column-one div.leftColumnButtons a:hover {
	text-decoration: none;
	color: &dtml-leftLinkTextHoverColor;;
}

ul.leftColumnButtons {
	margin: 30px 15px;
}

ul.leftColumnButtons li {
	display: block;
	padding: 0px;
	background: &dtml-leftButtonArrowBackground_external; url(&dtml-portal_url;/button_bullet.gif) no-repeat 4px center;
	border: 1px solid &dtml-leftButtonBackgroundBorder_external;;
	margin-bottom: 15px;
	list-style-type: none;
	list-style-image: none;
}

ul.leftColumnButtons li strong {
	padding: 0px;
	margin: 0px;
	border: 1px solid #FFFFFF;
	display: block
}

#portal-column-one ul.leftColumnButtons li a {
	text-decoration: none;
	display: block;
	color: &dtml-leftButtonTextColor_external;;
	font-weight: normal;
	font-size: 1.1em;
	margin-left: 16px;
	padding: 8px;
	background: &dtml-leftButtonBackground_external;;
	border-left: 1px solid #FFFFFF;
}

#portal-column-one ul.leftColumnButtons li.internal-link a {
	color: &dtml-leftButtonTextColor_internal;;
	background: &dtml-leftButtonBackground_internal;;
}

ul.leftColumnButtons li.internal-link {
	background-color: &dtml-leftButtonArrowBackground_internal;;
	border-color: &dtml-leftButtonBackgroundBorder_internal;;
}

.image-left, .image-right {
	border: 0px;
}

/* Paragraph and Unordered List readability per cmore */

.documentContent ul {
	margin:0.75em 0 0 2.5em;
}

p {
	margin: 1em 0;
}

/* Photo Album */
.photoAlbumEntry {
	background-image: none;
	padding: 0px 5px;
	width: 142px;
}


/* PloneFormGen Fields */

#pfg-fieldwrapper .field {
	clear: none;
}

/* Remove dashed underline from date abbr */
abbr.dtstart, abbr.dtend {
	border-bottom: 0px;
}

/* Give a dt.nodescription a bottom margin of 1em, since we're removing the dd if it doesn't have a description. */
dt.nodescription {
	margin-bottom: 1em;
}

span.description {
	display: block;
	font-size: 0.9em;
	line-height: 1.4em;
	margin-top: 0.25em;
}

.facultyStaffDirectory table.listing {
	border: 1px solid &dtml-globalBorderColor;;
}

/* Homepage Image overrides */
#homepageimage h1, #homepageimage h2 {
	display: block;
	padding: 15px 10px 5px 15px;
	margin: 0;
	background: transparent;
}

#homepageimage h1 {
	font-size: 1.9em;
	font-weight: 500;
	color: &dtml-homepageH1TextColor;;
	font-family: Georgia, Times New Roman, serif;
	letter-spacing: 0.06em;
}

#homepageimage h2 {
	font-size: 1.0em;
	font-weight: normal;
	color: &dtml-homepageH2TextColor;;
	font-family: Georgia, Times New Roman, serif;
	letter-spacing: 0.07em;
}

#homepageimage .text {
	position: absolute;
	margin: 0;
	padding: 0;
	width: 200px;
	z-index: 10;
}

#homepageimage .overlay {
	width: 200px;
	height: 265px;
	margin: 0px;
	padding: 0;
	background: &dtml-homepageTransparentOverlayColor;;
	opacity: 0.70;
	filter: alpha(opacity = 70);
	z-index: 5;
	position: absolute;
}

body.template-news_listing .effectiveDate,
body.template-folder_leadimage_listing .effectiveDate,
body.template-folder_leadimage_view .effectiveDate
 {
	margin-bottom: 0.1em;
	margin-top: 0.4em;
}

body.template-news_listing dd {
	margin-bottom: 2em;
	margin-left: 0;
}

/* Fix rightcolumn issue */

body.template-news_listing #rightcolumn dd {
    margin-bottom: 0;
}


body.template-newsitem_view .effectiveDate {
	color: #76797C;
	font-size: 0.9em;
	margin-bottom: 1em;
	margin-top: 0;
}

body.template-document_homepage_view #centercolumn dl.portletfeedmixer {
	clear: left;
}

body.template-atct_album_view .contentLeadImageContainer, body.template-pgv_sg_carousel_view  .contentLeadImageContainer {
	display: none;
}

/* Restyle pull quote */

.pullquote {
    border-width: 2px 0px;
    border-style: solid;
    border-color: #CC9933;
    color: #9D7627;
    padding: 1em;
    font-size: 1.3em;
    line-height: 1.5em;
}

.eventDetails {
	margin-top: 1.5em;
}

/* Link Icons */

#portal-column-one .linkIcon {
	margin: 30px 8px 20px 8px;
}

#portal-column-one .linkIcon li {
	margin: 1em 0;
}

#portal-column-one .linkIcon a {
	text-decoration: none;
	color: &dtml-leftLinkTextColor;;
}

#portal-column-one .linkIcon a:hover {
	text-decoration: none;
	color: &dtml-leftLinkTextHoverColor;;
}


/* Link Buttons */

#portal-column-one .linkButton ul {
	margin: 30px 15px;
}

#portal-column-one .linkButton li {
	display: block;
	padding: 0px;
	background: &dtml-leftButtonArrowBackground_external; url(&dtml-portal_url;/button_bullet.gif) no-repeat 4px center;
	border: 1px solid &dtml-leftButtonBackgroundBorder_external;;
	margin-bottom: 15px;
	list-style-type: none;
	list-style-image: none;
}

#portal-column-one .linkButton strong {
	padding: 0px;
	margin: 0px;
	border: 1px solid #FFFFFF;
	display: block
}

#portal-column-one .linkButton a {
	text-decoration: none;
	display: block;
	color: &dtml-leftButtonTextColor_external;;
	font-weight: normal;
	font-size: 1.1em;
	margin-left: 16px;
	padding: 8px;
	background: &dtml-leftButtonBackground_external;;
	border-left: 1px solid #FFFFFF;
}

#portal-column-one .linkButton li.internal-link a {
	color: &dtml-leftButtonTextColor_internal;;
	background: &dtml-leftButtonBackground_internal;;
}

#portal-column-one .linkButton li.internal-link {
	background-color: &dtml-leftButtonArrowBackground_internal;;
	border-color: &dtml-leftButtonBackgroundBorder_internal;;
}

.template-fg_base_view_p3 textarea {
	width: 450px;
}

/* Removing avatar from quintasgroup comments */

div.comment img.avatar {
	display: none;
}

/* Content Well Portlets for Sitemap */

#portlets-below {
	background: &dtml-sitemapBackgroundColor;;
	font-size: 0.88em;
	margin-right: 6px;
	margin-left: 6px;
	margin-top: 0px;
	padding-bottom: 6px;
}

#portlets-below div.portletsBelowContentF {
	clear: both;
	height: 1px;
}

div.portletsBelowContentA,
div.portletsBelowContentB,
div.portletsBelowContentC,
div.portletsBelowContentD,
div.portletsBelowContentE {
	width: 20%;
	float: left;
}

#portlets-below .portlet, #portlets-below .even, #portlets-below .portletFooter,
#portlets-below .portletItem, #portlets-below .portletHeader {
	background: transparent;
	border: 0;
}

#portlets-below dl.portlet {
    padding: 0.5em 0 0.5em 0.75em;
    margin: 0.5em;
}

#portlets-below .portletHeader {
	text-transform: uppercase;
	font-size: 1.1em;
	font-weight: bold;
	padding: 0.5em 0em 0.25em 5px;
}

#portlets-below dd.portletItem
{
	padding: 0;
    margin: 0;
}

#portlets-below .portletNavigationTree dd.portletItem
{
    padding: 0;
}

#portlets-below .navTree li
{
	padding: 0;
    margin: 0;
}

#portlets-below .portletHeader a, #portlets-below .portletHeader span {
	color: &dtml-sitemapHeaderColor; !important;
	padding: 0;
}

#portlets-below .portletFooter {
	text-align: left;
	padding: 0.25em 0;
}

#portlets-below .portletFooter a {
	color: &dtml-sitemapLinkColor;;
	display: block;
	background: transparent;
}

#portlets-below a, #portlets-below .state-published
{
	color: &dtml-sitemapLinkColor; !important;
	padding: 0.25em 5px;
	display: block;
}

#portlets-below .portletItem .state-published:hover,
#portlets-below .portletItem a:hover {
	color: &dtml-sitemapBackgroundColor; !important;
	background: &dtml-sitemapLinkColor; !important;
}


/* Navigation Portlet */

#portlets-below dd.portletItem .navTreeItem a, 
#portlets-below .navTreeItem a,
#portlets-below .navTreeCurrentItem
{
    background: transparent;
    border: 0 !important;
    font-weight: normal;
}


/* RSS Feedmixer portlet in centercolumn */
body.template-document_homepage_view #centercolumn dl.portletfeedmixer .portletHeader {
    display: block;
    margin: -5px -7px 0px -8px;
    font-size: 0.88em;
    font-weight: bold;
    text-align: left;
    text-transform: uppercase;
    border: 0;
    padding: 4px 9px 4px 11px;
}

body.template-document_homepage_view #centercolumn .portletfeedmixer dd.first {
	border-width: 0px 0px 0px 0px;
	padding-top: 3px;
}

body.template-document_homepage_view #centercolumn {
    font-size: 0.92em;
    margin-bottom: 3px;
}

body.template-document_homepage_view #centercolumn dl.portletfeedmixer .portletItem {
    font-size: 1.1em;
}


/* Configure Plone Portal Column Two */

#portal-column-two {
	padding: 1em 0.5em 0 0.5em;
	margin: 0 0 0 3px;
	float: right;
	background: &dtml-insideRightPortletBackgroundColor;;
	width: 16em;
	border-width: 0 0 0 2px;
	border-color: &dtml-insideRightPortletBackgroundColor;;
	border-style: solid;
}

#portal-column-two .visualPadding {
    padding: 0;
}

#portal-column-two>div {
	margin: 0px 0px 30px 0px;
}

#portal-column-two .portletWrapper {
	background: transparent;
	border: none;	
	padding: 0;
	margin-bottom: 20px;
	font-size: 1em;
}

#portal-column-two .portlet {
	border-width: 0px;
	margin: 0px 0px 0px auto;
	background: transparent;
}

#portal-column-two .portletHeader {
	background: transparent; 
	color: &dtml-insideRightPortletHeaderTextColor;; 
	font-size: 1em;
	font-weight: bold;
	padding: 2px 0px;
	text-align: left;
	text-transform: uppercase;
	border-width: 0px 0px 1px 0px;
	border-style: solid;
	border-color: &dtml-insideRightPortletHeaderTextColor;; 
	margin: 2px 6px 0 6px; 
}

#portal-column-two .portletHeader a {  
	color: &dtml-insideRightPortletHeaderTextColor;;
}

#portal-column-two .portletItem {
	background: transparent;
	border-width: 0px;
	color: &dtml-insideRightPortletTextColor;;
}

#portal-column-two .portletItem ul {
	margin: 0px;
	padding: 0px;
	float: none;
}

#portal-column-two .portletItem li {
	background: transparent url(&dtml-portal_url;/blue_bullet.gif) no-repeat scroll 0 4px;
	list-style-image: none;
	list-style-type: none;
	margin: 2px 5px 5px;
	padding: 1px 0 1px 13px;
	font-size: 1.00em;
	color: &dtml-insideRightPortletTextColor;;
}

#portal-column-two .portletItem a {
	color: &dtml-insideRightPortletLinkColor;;
}

#portal-column-two .portletItem a:hover {
	color: &dtml-insideRightPortletLinkHoverColor;;
}

#portal-column-two .portletFooter {
	background: &dtml-insideRightPortletBackgroundColor;; 
}

#portal-column-two ul li a {
	color: &dtml-insideRightPortletLinkColor;;
	font-size: 1.00em;
	text-decoration: none;
	border: 0px;
	padding: 0px;
}

#portal-column-two .navTreeCurrentItem {
    border: 0 !important;
    font-weight: bold;
    background: transparent;
}

/* End Portal Column Two */



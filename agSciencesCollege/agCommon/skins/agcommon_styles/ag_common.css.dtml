	padding: 0 0.75em;
    margin-right:0.25em;
}

.documentActions {
	margin-top: 20px;
}

.documentContent {
	padding-bottom: 0px !important;
}

.documentContent dd a {
	border: none;
}

h1.documentFirstHeading, .facultyStaffDirectory h1, #region-content h1 {

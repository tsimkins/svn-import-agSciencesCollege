	background-image: none;
	padding: 0px 5px;
	width: 142px;
}


/* PloneFormGen Fields */

#pfg-fieldwrapper .field {
	clear: none;
}

/* Remove dashed underline from date abbr */
abbr.dtstart, abbr.dtend {
	border-bottom: 0px;
}

/* Give a dt.nodescription a bottom margin of 1em, since we're removing the dd if it doesn't have a description. */
dt.nodescription {
	margin-bottom: 1em;
}

span.description {
	display: block;
	font-size: 0.9em;
	line-height: 1.25em;
	margin-top: 0.25em;
}

.facultyStaffDirectory table.listing {
	border: 1px solid &dtml-globalBorderColor;;
}

/* Homepage Image overrides */
#homepageimage h1, #homepageimage h2 {
	display: block;
	padding: 15px 10px 5px 15px;
	margin: 0;
	background: transparent;
}

#homepageimage h1 {
	font-size: 1.9em;
	font-weight: 500;
	color: &dtml-homepageH1TextColor;;
	font-family: Georgia, Times New Roman, serif;
	letter-spacing: 0.06em;
}

#homepageimage h2 {
	font-size: 1.0em;
	font-weight: normal;
	color: &dtml-homepageH2TextColor;;
	font-family: Georgia, Times New Roman, serif;
	letter-spacing: 0.07em;
}

#homepageimage .text {
	position: absolute;
	margin: 0;
	padding: 0;
	width: 200px;
	z-index: 10;
}

#homepageimage .overlay {
	width: 200px;
	height: 265px;
	margin: 0px;
	padding: 0;
	background: &dtml-homepageTransparentOverlayColor;;
	opacity: 0.70;
	filter: alpha(opacity = 70);
	z-index: 5;
	position: absolute;
}



/* YOUR CSS RULES START HERE */



body {
	background: &dtml-bodyBackgroundColor;;
	color: #333333;
}

#background-stripe-one, 
#background-stripe-two, 
#background-stripe-three {
	margin:0;
	padding:0;
	border:0;
	position:absolute;
	width: 100%;
	z-index: -10;
	left: 0;
}

#background-stripe-one {
	background: &dtml-backgroundStripeOneColor;;
	top: 0px;
	height: 98px;
	z-index: -15;
}
#background-stripe-two {
	background: &dtml-backgroundStripeTwoColor;;
	top: 65px;
	height: 10px;
}
#background-stripe-three {
	background: &dtml-backgroundStripeThreeColor;;
	top: 75px;
	height: 23px;
}
#content a, #portal-column-one a{
	color: #003399;
}

#content a:hover, #portal-column-one a:hover {
	color: #3366CC;
}

dt {
	font-weight: normal;
}

#visual-portal-wrapper {
	background: #FFFFFF;
	margin: 0px auto;
	padding-bottom: 6px;
	max-width: &dtml-maxWidth;;
	min-width: &dtml-minWidth;;
}

#portal-searchbox {
	margin: 4px;
}

#portal-content-wrapper {
	float: left;
	background: #FFFFFF;
	padding-bottom: 20px;
}

#portal-header {
	background: #FFFFFF url(&dtml-portal_url;/&dtml-logoName;) no-repeat left bottom;
	height: 76px;
	margin: 0;
	padding: 0;
}

#logo-print {
	display: none;
}

#portal-top, #portal-columns, #portal-footer, #clear-space-before-footer {
	margin: 0px 6px;
	border-left: 1px solid #808080;
	border-right: 1px solid #808080;
}

#portal-breadcrumbs {
	background-color: &dtml-breadcrumbBackgroundColor;;
	color: &dtml-breadcrumbTextColor;;
	font-size: 0.92em;
	font-weight: bold;
	letter-spacing: 0.025em;
	padding: 3px 3px;
	border-bottom: 0px;
}

#portal-breadcrumbs .breadcrumbSeparator {
	color: &dtml-breadcrumbSeparatorColor;;
}

#portal-breadcrumbs a {
	color: &dtml-breadcrumbLinkColor;;
	font-weight: normal;
}

#portal-column-content {
	margin-bottom: 0px;
	padding: 0 0 0 0px;
	background: #FFFFFF;
}


#portal-columns>div.visualPadding {
	float: left;
	width: 187px;
}
/*
#portal-column-content.sr, #portal-column-content.sl {
	float: left;
	margin-right: 0;
	width: 650px;
	margin-left: 187px;
}

*/


#portal-column-content.sr, #portal-column-content.sl {
	margin-left: 187px;
}


/* ------------ PORTLETS --------------*/

#portal-column-one .visualPadding {
	padding: 0px; 
}

#portal-column-one .portlet, #portal-column-one .portletItem {
	border: none;
}

#portal-column-one .portletItem {
	padding: 0px;
}

#portal-column-one .portletFooter {
	display: none;
}

.portletItemDetails {
	color: #333333;
	font-size: 0.92em;
}

#portal-column-one .portletNavigationTree .portletHeader {
	display: none;
}


#portal-column-one .portletNavigationTree .portletItem ul {
	margin: 5px;
}

#portal-column-one .portletNavigationTree .portletItem li {
	border-bottom: 1px dashed #CCCCCC;
	font-size: 1em;
	margin: 6px 5px;
	padding-bottom: 5px;
}


#portal-column-one .portletNavigationTree .portletItem li a {
	font-size: 1.1em;
}

#portal-column-one ul.navTreeLevel1 {
	background-color: &dtml-leftSubNavBackground;;
	border: #DDD solid 1px;
}

#portal-column-one .navTreeCurrentItem, a.navTreeItemInPath {
	border-width: 0px !important;
	font-weight: bold;
}

#portal-column-one ul.navTreeLevel1 .navTreeCurrentItem {
	background-color: &dtml-leftSubNavBackground;;	
}

#portal-column-one .navTreeItem a, #portal-column-one .navTreeItem a:hover, #portal-column-one dd.portletItem #portal-column-one .navTreeItem a, 
#portal-column-one dd.portletItem .navTreeItem a:hover {
	border-width: 0px;
	background-color: transparent;
	color: &dtml-leftNavTextColor;;
}

#portal-column-one .navTreeItem a:hover, #portal-column-one 
dd.portletItem .navTreeItem a:hover {
	color: &dtml-leftNavTextHoverColor;;
}

dd.portletItem {
	padding: 0.7em;
}

#portal-column-one ul.navTreeLevel1 .navTreeItem a, #portal-column-one ul.navTreeLevel1 .navTreeItem a:hover, #portal-column-one ul.navTreeLevel1 
dd.portletItem .navTreeItem a, #portal-column-one ul.navTreeLevel1 dd.portletItem .navTreeItem a:hover {
	background-color: &dtml-leftSubNavBackground;; 
	color: &dtml-leftNavTextColor;;;
}

#portal-column-one ul.navTreeLevel1 .navTreeItem a:hover, 
#portal-column-one ul.navTreeLevel1 dd.portletItem .navTreeItem a:hover
{
	color: &dtml-leftNavTextHoverColor;;
}


ul.navTreeLevel1 li.navTreeItem {
	background: &dtml-leftSubNavBackground; url(&dtml-portal_url;/blue_bullet.gif) no-repeat 3px 6px;
	padding: 0px 5px 0px 14px;
}

#portal-column-one .portletNavigationTree .portletItem li li, #portal-column-one .navTreeItem a {
	border: none;
}
#portal-column-one #portal-column-one-visualPadding.visualPadding {
	background: transparent url(&dtml-portal_url;/&dtml-bottomLeftColumnBackgroundImage;) no-repeat left bottom;
	padding: 0 0 150px 0;
}

#portal-column-one {
	background: &dtml-leftNavBackgroundColor;;
	border-color: #808080;
	border-style: solid;
	border-width: 0 1px 0 0;
/*	min-height: 375px; */
	padding: 2px 0px 0px 0px;
	width: 186px;
	float: left;
	margin: 0px 0px 0px 0px;
}

#portal-column-one .portlet {
	background: &dtml-leftNavBackgroundColor;;
}

/* -------- MAIN NAV Portlet --------*/


/*-------- FOOTER -------------------*/

#portal-footer {
	background-color: &dtml-footerBackgroundColor;;
	color: &dtml-footerTextColor;;
	border-width: 0px;
}

#portal-footer a, #portal-footer a:visited {
	color: &dtml-footerLinkColor;;
}

/* Added By Tim 2008/07/30 */

#breadcrumbs-you-are-here {
	display: none;
}

.searchSection {
	display: none;
}

#contact p {
	margin: 0px;
}

#contact a:hover {
	background: &dtml-contactColor; none repeat scroll 0 0;
	color: &dtml-contactBackgroundColor;;
}

#contact a {
	background: &dtml-contactBackgroundColor; none repeat scroll 0 0;
	border-width: 0 1px 0 0;
	border-style: solid;
	border-color: #808080;
	color: &dtml-contactColor;; 
	display: block;
	font-size: 0.88em;
	font-weight: bold;
	margin: 0px 0px 0px 0px;
	padding: 10px 0;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
/*	float: left; */
	width: 186px;
}


/* topnavigation */

#topnavigation {
	background: &dtml-topnavigationBackgroundColor;;
	margin: 0px 0px 0px 0px;
	padding: 0px;
}

#topnavigation ul {
	display: block;
	text-align: left;
	margin: 0px auto;
	padding: 0 0 0 4px;
	border: 0px;
	list-style-type: none;
}

#topnavigation li {
	background: &dtml-topnavigationButtonBackgroundColor;;
	border-color: &dtml-topnavigationBackgroundColor;;
	border-width: 0px 0px 0px 1px;
	border-style: solid;
	display: -moz-inline-box;
	display: inline-block;
	padding: 3px 4px;
	margin: 3px 0px;
	white-space: nowrap;	
}

#topnavigation li:first-child {
	border-width: 0px;
}

#topnavigation li a {
	display: inline;
	font-size: 1em;
	color: &dtml-topnavigationTextColor;;
	text-decoration: none;
	border: 0px;
}

#topnavigation img {
	display: none;
}

#topnavigation .portletHeader {
	display: none;
}

#topnavigation .portletItem {
	padding: 0;
	border: 0px;
}

#topnavigation .portlet {
	background-color: transparent;
	border: 0px;
}

#topnavigation li.navTreeItem div {
	display: inline;
}

.navTreeItem a:hover,
dd.portletItem .navTreeItem a:hover {
	background: transparent;
}

.documentEditable .contentActions,
.documentEditable .contentViews,
.documentEditable .documentContent
{
	margin: 0px 5px;
}

.documentEditable .contentViews { 
	margin-top: 20px;
}


.documentActions {
	margin-top: 20px;
}

.documentContent {
	padding-bottom: 0px !important;
}

.documentContent dd a {
	border: none;
}

h1.documentFirstHeading, .facultyStaffDirectory h1, #content h1 {
	color: &dtml-documentTitleColor;;
	font-weight: normal;
	border: none;
}

#clear-space-before-navigation {
	clear: none;
}

/* No H1 on home page */
body.template-document_homepage_view h1 {
	display: none;
}

body.template-document_homepage_view #centercolumn h1.documentFirstHeading {
	display: block;
}

body.template-document_homepage_view .documentActions {
	display: none;
}

/* Configure Right Column Portlets */

#rightcolumn {
	float: right;
	padding: 0px;
	margin: 0px;
	position: relative;
}

#rightcolumn>div {
	margin: 0px 0px 30px 20px;
}

#rightcolumn .portletWrapper {
	width: 175px;
	background: &dtml-insideRightPortletBackgroundColor; none repeat scroll 0 0;
	border: 1px solid #CCCCCC;	
	padding: 1px;
	margin-bottom: 20px;
	font-size: 0.88em;
}

#rightcolumn .portlet {
	border-width: 0px;
	margin: 0px 0px 0px auto;
}

#rightcolumn .portletHeader {
	background: &dtml-insideRightPortletHeaderBackgroundColor;; 
	color: &dtml-insideRightPortletHeaderTextColor;; 
	font-size: 1em;
	font-weight: bold;
	padding: 4px;
	text-align: center;
	text-transform: uppercase;
	border-width: 0px;
	margin: 0px; 

}

#rightcolumn .portletHeader a {  
	color: &dtml-insideRightPortletHeaderTextColor;;
}

#rightcolumn .portletItem {
	background: &dtml-insideRightPortletBackgroundColor;;
	border-width: 0px;
}


#rightcolumn .portletItem ul {
	margin: 0px;
	padding: 0px;
	float: none;
}

#rightcolumn .portletItem li {
	background: transparent url(&dtml-portal_url;/blue_bullet.gif) no-repeat scroll 0 5px;
	list-style-image: none;
	list-style-type: none;
	margin: 2px 5px 5px;
	padding: 1px 0 1px 13px;
	font-size: 1.00em;
}

#rightcolumn .portletFooter {
	background: &dtml-insideRightPortletBackgroundColor;; 
}


#rightcolumn ul li a {
	color: #003399;
	font-size: 1.00em;
	text-decoration: none;
	border: 0px;
	padding: 0px;
}

/* Right Column Front Page modifications */

body.template-document_homepage_view #rightcolumn {
	float: right;
	margin: 1px -9px 0px -9px;
	padding: 0px;
	font-weight: normal;
	font-size: 0.92em;
	background: &dtml-frontRightPortletBackgroundColor;;
}

body.template-document_homepage_view #rightcolumn .portletHeader, body.template-document_homepage_view #rightcolumn .portletHeader a {

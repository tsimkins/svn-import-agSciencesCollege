}

/* topnavigation */

#topnavigation {
	background: &dtml-topnavigationBackgroundColor;;
	margin: 0px 0px 0px -1px;
	padding: 0px;
}

#topnavigation ul {
	display: block;
	text-align: left;
	margin: 0px auto;
	padding: 0 0 0 4px;
	border: 0px;
	list-style-type: none;
}

#topnavigation li {
	background: &dtml-topnavigationButtonBackgroundColor;;
	border-color: &dtml-topnavigationBackgroundColor;;
	border-width: 0px 0px 0px 1px;
	border-style: solid;
	display: -moz-inline-box;
	display: inline-block;
	padding: 3px 4px;
	margin: 3px 0px;
	white-space: nowrap;	
}

#topnavigation li:first-child {
	border-width: 0px;
}

#topnavigation li a {
	display: inline;
	font-size: 1em;
	color: &dtml-topnavigationTextColor;;
	text-decoration: none;
	border: 0px;
}

#topnavigation img {
	display: none;
}

#topnavigation .portletHeader {
	display: none;
}

#topnavigation .portletItem {
	padding: 0;
	border: 0px;
}

#topnavigation .portlet {
	background-color: transparent;
	border: 0px;
}

#topnavigation li.navTreeItem div {
	display: inline;
}

.navTreeItem a:hover,
dd.portletItem .navTreeItem a:hover {
	background: transparent;
}

.documentEditable .contentActions,
.documentEditable .contentViews,
.documentEditable .documentContent
{
	margin: 0px 5px;
}

.documentEditable .contentViews { 
	margin-top: 20px;
}

/* Squish tabs in edit view together when we have a right column */

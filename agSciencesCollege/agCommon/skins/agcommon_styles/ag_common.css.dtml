	border-style: solid !important;
}

body.template-document_homepage_view #centercolumn .navTreeItem a:hover, 
body.template-document_homepage_view #centercolumn .navTreeItem a, 
body.template-document_homepage_view #centercolumn .navTreeCurrentItem a, 

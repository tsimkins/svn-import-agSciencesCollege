	text-decoration: none;
	border: 0px;
}

body.template-document_homepage_view #rightcolumn .portletItem a:hover,
body.template-document_homepage_view #centercolumn .portletItem a:hover {
	color: #3366CC;
}

/* Configure Homepage Image Portlet */

#homepageimage {

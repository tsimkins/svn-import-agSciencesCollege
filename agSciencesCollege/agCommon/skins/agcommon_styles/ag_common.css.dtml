/* Configure Right Column Portlets */

#rightcolumn {
	float: right;
	padding: 0px;
	margin: 0px;
	position: relative;
	clear: both;
    display: none; 
}

#rightcolumn>div {
	margin: 0px 0px 30px 20px;
}

#rightcolumn .portletWrapper {
	width: 175px;
	background: &dtml-insideRightPortletBackgroundColor; none repeat scroll 0 0;
	border: 1px solid #CCCCCC;	
	padding: 1px;
	margin-bottom: 20px;
	font-size: 0.88em;
}

#rightcolumn .portlet {
	border-width: 0px;
	margin: 0px 0px 0px auto;
}

#rightcolumn .portletHeader {
	background: &dtml-insideRightPortletHeaderBackgroundColor;; 
	color: &dtml-insideRightPortletHeaderTextColor;; 
	font-size: 1em;
	font-weight: bold;
	padding: 4px;
	text-align: center;
	text-transform: uppercase;
	border-width: 0px;
	margin: 0px; 
}

#rightcolumn .portletHeader a {  
	color: &dtml-insideRightPortletHeaderTextColor;;
}

#rightcolumn .portletItem {
	background: &dtml-insideRightPortletBackgroundColor;;
	border-width: 0px;
	color: &dtml-insideRightPortletTextColor;;
}

#rightcolumn .portletItem ul {
	margin: 0px;
	padding: 0px;
	float: none;
}

#rightcolumn .portletItem li {
	background: transparent url(&dtml-portal_url;/blue_bullet.gif) no-repeat scroll 0 5px;
	list-style-image: none;
	list-style-type: none;
	margin: 2px 5px 5px;
	padding: 1px 0 1px 13px;
	font-size: 1.00em;
	color: &dtml-insideRightPortletTextColor;;
}

#rightcolumn .portletItem a {
	color: &dtml-insideRightPortletLinkColor;;
}

#rightcolumn .portletItem a:hover {
	color: &dtml-insideRightPortletLinkHoverColor;;
}

#rightcolumn .portletFooter {
	background: &dtml-insideRightPortletBackgroundColor;; 
}

#rightcolumn ul li a {
	color: &dtml-insideRightPortletLinkColor;;
	font-size: 1.00em;
	text-decoration: none;
	border: 0px;
	padding: 0px;
}

#rightcolumn .navTreeCurrentItem {
    border: 0 !important;
    font-weight: bold;
}

/* Right Column Front Page modifications */


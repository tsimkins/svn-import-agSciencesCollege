/* YOUR CSS RULES START HERE */

#announcement-box {
    border: #999999 1px solid;
    background-color: #dfbc1b;
    width: 146px;
    padding: 3px;
}

#announcement-box p {
    font-weight: bold;
    color: #233b8f;
    margin: 0.25em 0;
    font-size: 1em;
    line-height: 1.1em;
}

#announcement-box input{
    font-family: Verdana, Geneva, Arial, Helvetica, san-serif;
    margin: 0.25em 0;
    font-size: 0.88em;
}

#edit_county_tags, #edit_main_tags {
    width: 120px;
    display: block;
    border: 1px solid #FF981F;
    background-color: #EF9C3B;
    margin-top: 3px;
    padding: 3px;
    font-size: 0.88em;
    text-align: center;
    color: #FFFFFF !important;
    text-decoration: none;
}

/* --------------------------- Customizations to front page portlets -------------------------------- */

body.section-front-page #portlets-above .portlet {
    min-height: 32em;
}

body.section-front-page #portlets-above dl.portlet:first-child .portletHeader {
    padding-top: 50px;
}

body.section-front-page h1.documentFirstHeading {
    display: none;
}

body.section-front-page #region-content h2.documentDescription {
    font-size: 1.5em;
    margin: 0 0 0.25em 0;
}

body.section-front-page #region-content p {
    margin: 0 0 2em 0;
}

body.section-front-page #portlets-below {
    display: none;
}

/* --------------------------- Front page portlet graphics -------------------------------- */

/* Testing */

body.section-front-page #portlets-above .portletHeader {
    font-size: 12px;
}

body.section-front-page #portlets-above .portletHeader a, #portlets-above .portletHeader span {
    color:#FCFCFC !important;
}

body.section-front-page #portlets-above .portletHeader a {
    padding: 1px 0 1px 5px;
    display: block;
    width: 195px;
    background: &dtml-topnavigationGradientBackground;;
}


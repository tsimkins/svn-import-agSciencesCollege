/* YOUR CSS RULES START HERE */

#edit_county_tags, #edit_main_tags {
    width: 120px;
    display: block;
    border: 1px solid #FF981F;
    background-color: #EF9C3B;
    margin-top: 3px;
    padding: 3px;
    font-size: 0.88em;
    text-align: center;
    color: #FFFFFF !important;
    text-decoration: none;
}

/* --------------------------- Customizations to front page portlets -------------------------------- */

body.section-front-page #portal-column-one {
    border: none;
}

body.section-front-page #portlets-above .portlet {
    min-height: 20em;
    margin-top: 0.375em;
}

body.section-front-page #portlets-above dl.portlet:first-child .portletHeader {
    padding-top: 50px;
}

body.section-front-page h1.documentFirstHeading {
    display: none;
}

body.section-front-page #content #homepage-text p.documentDescription {
    font-size: 1.25em;
    line-height: 1.375em;
    margin-right: 0.25em;
}

body.section-front-page #content p {
    margin: 0 0 2em 0;
}

body.section-front-page #portlets-below {
    display: none;
}

/* --------------------------- Front page portlet graphics -------------------------------- */

/* Testing */

body.section-front-page #portlets-above .portletHeader {
    font-size: 1.0625em;
    margin: 0 !important;
    padding: 5px 0 3px 8px !important;
}

body.section-front-page #portlets-above .portletHeader, #portlets-above .portletHeader span {
    color:#FCFCFC !important;
}

body.section-front-page #portlets-above .portletHeader {
    display: block;
    background: &dtml-topnavigationGradientBackground;;
    font-family: Arial, Helvetica, Helv, sans-serif;
    letter-spacing: 0.0125em;
}

body.section-front-page #portlets-above .portletWrapper {
    background-repeat: no-repeat;
    padding-top: 50px;
    background-size: 100% 50px;
}

body.section-front-page #content #portlets-above .portletItem a,
body.section-front-page #content #portlets-above .portletItem a:hover {
    padding-left: 8px !important;
    padding-top: 0.5em !important;
    border-bottom: none;
}


<metal:content-core fill-slot="content-core">
    <metal:block define-macro="content-core"
                        tal:define="kssClassesView context/@@kss_field_decorator_view;
                                    getKssClasses nocall:kssClassesView/getKssClassesInlineEditable;
                                    standalone_view python:True;
                                    templateId template/getId;">

        <metal:featured tal:define="batch view/getEnabledItems">
            <metal:listingmacro define-macro="listing">
                <tal:foldercontents define="notEditor not:view/canEdit;
                                    contentFilter contentFilter|request/contentFilter|nothing;
                                    contentFilter python:contentFilter and dict(contentFilter) or {};
                                    limit_display limit_display|request/limit_display|nothing;
                                    limit_display python:limit_display and int(limit_display) or None;
                                    more_url more_url|request/more_url|string:folder_contents;
                                    is_a_topic python:context.portal_type=='Topic';
                                    folderContents folderContents|nothing;
                                    folderContents python:view.folderContents(folderContents=folderContents, contentFilter=contentFilter);
                                    site_properties context/portal_properties/site_properties;
                                    use_view_action site_properties/typesUseViewActionInListings|python:();
                                    Batch python:modules['Products.CMFPlone'].Batch;
                                    b_start python:request.get('b_start', 0);
                                    isAnon context/@@plone_portal_state/anonymous;
                                    normalizeString nocall: context/plone_utils/normalizeString;
                                    toLocalizedTime nocall: context/@@plone/toLocalizedTime;
                                    show_about python:not notEditor or site_properties.allowAnonymousViewAbout;
                                    show_date context/show_date|nothing;
                                    navigation_root_url context/@@plone_portal_state/navigation_root_url;
                                    pas_member context/@@pas_member;
                                    leadimage_full_width context/leadimage_full_width|nothing;
                                    newsletter_full_text context/newsletter_full_text|nothing"
                                    condition="notEditor">
                    <tal:listing condition="batch">

                        <metal:featured-articles define-slot="featured-articles">
                            <!-- empty -->
                        </metal:featured-articles>
                        <tal:list metal:define-slot="entries">
                            <tal:entry tal:repeat="item batch" metal:define-macro="entries">
                            <tal:block tal:define="item_object item/getObject;
                                                   item_has_leadimage item/hasContentLeadImage;
                                                   item_url item/getURL|item/absolute_url;
                                                   item_id item/getId|item/id;
                                                   item_title_or_id item/pretty_title_or_id;
                                                   item_description item/Description;
                                                   item_type item/portal_type;
                                                   item_type_title item/Type;
                                                   item_modified item/ModificationDate;
                                                   item_effective item/EffectiveDate;
                                                   item_created item/CreationDate;
                                                   item_icon python:plone_view.getIcon(item);
                                                   item_type_class python:'contenttype-' + normalizeString(item_type);
                                                   item_wf_state item/review_state|python: context.portal_workflow.getInfoFor(item, 'review_state', '');
                                                   item_wf_state_class python:'state-' + normalizeString(item_wf_state);
                                                   item_creator item/Creator;
                                                   item_tags item/Subject|nothing;
                                                   item_start item/start|item/StartDate|nothing;
                                                   item_end item/end|item/EndDate|nothing;
                                                   item_sametime python: item_start == item_end;
                                                   item_samedate python: (item_end - item_start &lt; 1) if item_type == 'Event' else False">
                                <metal:block define-slot="entry">
                                <tal:item metal:define-macro="listitem">
                                    <tal:image tal:condition="leadimage_full_width">
                                        <p class="leadimage" tal:condition="exists:item_has_leadimage">
                                            <a tal:attributes="href item_url">
                                                <img tal:replace="structure python:view.tag(item_object, scale='preview', css_class='leadimage_full')" border="0"/>
                                            </a>
                                        </p>
                                    </tal:image>

                                    <h3 class="tileHeadline">

                                    <tal:image
                                         tal:define="scale python: newsletter_full_text and 'mini' or 'micro'"
                                         tal:condition="not:leadimage_full_width">
                                            <img align="right"
                                                 tal:condition="exists:item_has_leadimage"
                                                 tal:replace="structure python:view.tag(item_object, scale=scale, css_class='leadimage')" />
                                    </tal:image>

                                    <span tal:condition="python: not view.isSpotlight(item) and view.showSummary" tal:content="string:${repeat/item/number}." />

                                        <a href="#"
                                           tal:attributes="href item_url;
                                                           class string:$item_type_class $item_wf_state_class url utm_newsletter_title"
                                           tal:content="item_title_or_id">
                                            Item Title
                                        </a>

                                    </h3>

                                <div tal:condition="python: item_type in ['Event', 'TalkEventType']" class="eventDateLocationBlock">
                                    <metal:event use-macro="context/folder_listing_z3/macros/event_date" />
                                </div>

                                <p tal:condition="item_description|nothing">

                                    <span tal:replace="item_description" />
                                    <tal:more condition="not: newsletter_full_text">
                                    [<a href="#"
                                        tal:attributes="href item_url;
                                                        class string:$item_type_class $item_wf_state_class url utm_newsletter_more"
                                        >more</a>]
                                    </tal:more>
                                </p>

                                <tal:fulltext condition="newsletter_full_text">
                                    <div class="documentText" 
                                         tal:condition="item_object/getText|nothing">
                                        <metal:field 
                                               use-macro="python:item_object.widget('text', mode='view')">
                                            Body text
                                        </metal:field>
                                    </div>
                                </tal:fulltext>

                                <br class="separator"
                                    tal:condition="not: context/leadimage_full_width|nothing" >

                                <hr class="separator"
                                    tal:condition="context/leadimage_full_width|nothing" >

                                </tal:item>
                            </metal:block>
                            </tal:block>
                            </tal:entry>
                        </tal:list>
                    </tal:listing>

                </tal:foldercontents>
            </metal:listingmacro>
        </metal:featured>
    </metal:block>
</metal:content-core>

<metal:email-footer fill-slot="email-footer">

    <metal:email-footer define-slot="email-footer">

        <div class="email-footer"
            tal:define="show_newsletter_footer context/show_newsletter_footer|nothing;"
            tal:condition="show_newsletter_footer"
            bgcolor="#ffffff" align="left">
    
        <hr size="1">
        
        <tal:listserv 
            define="listserv view/listserv|nothing;
                    listserv_subscribe_email view/listserv_subscribe_email|nothing;
                    listserv_unsubscribe_email view/listserv_unsubscribe_email|nothing;"
            condition="listserv">
                <h2>List Information</h2>
                <p>You received this email because you are subscribed to the <strong tal:content="listserv" /> mailing list. If you would like to unsubscribe from this list, simply send an email to 
                    <a class="no_utm"
                       tal:attributes="href string: mailto:${listserv_unsubscribe_email}"
                       tal:content="listserv_unsubscribe_email" />.
                    
                    No subject or message is necessary in the email.</p>
                
                <p>To subscribe to this list, simply send an email to  <a class="no_utm"
                       tal:attributes="href string: mailto:${listserv_subscribe_email}"
                       tal:content="listserv_subscribe_email" />.</p>
                
                <p>For any questions or problems, contact the 
                    <a class="no_utm" 
                       tal:attributes="href string: mailto:${view/listserv_contact_email}">list administrator</a>.</p>
        </tal:listserv>
            
        <h2>Mailing Address</h2>
        <p>
            <span tal:condition="is_extension" tal:replace="string: Penn State Extension" />
            <span tal:condition="not:is_extension" tal:replace="string: Penn State College of Agricultural Sciences" />
            <br />
            Agricultural Administration Building<br />
            University Park, PA  16802
        </p>
        </div>
                    
    </metal:email-footer>           

</metal:email-footer>

<metal:portlets define-slot="portlets_two_slot" fill-slot="portlets_two_slot">
    <tal:block replace="structure provider:plone.rightcolumn" />
</metal:portlets>

<metal:portlets define-slot="portlets_bottom_slot" fill-slot="portlets_bottom_slot">

</metal:portlets>

<metal:analytics define-slot="analytics_viewlet" fill-slot="analytics_viewlet">

</metal:analytics>

</body>
</html>
</metal:page>
